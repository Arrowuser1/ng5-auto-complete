{
  "name": "ng5-auto-complete",
  "version": "0.0.1",
  "description": "An Auto-Complete Model for Angular 5 with extended features and make life easy.",
  "main": "index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/autoComplete.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/autoComplete.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hardy12994/ng5-auto-complete.git"
  },
  "keywords": [
    "ng5-auto-complete",
    "auto-complete",
    "ng5",
    "auto-list",
    "ng-auto-complete",
    "autoComplete-ng5"
  ],
  "author": "hardeepsingh12994@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hardy12994/ng5-auto-complete/issues"
  },
  "homepage": "https://github.com/hardy12994/ng5-auto-complete#readme",
  "dependencies": {
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.5",
    "@angular/compiler-cli": "^5.2.5",
    "node-sass": "^4.5.3",
    "rollup": "^0.50.0",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.11.1",
    "typescript": "^2.7.2",
    "uglify-js": "^3.1.8"
  }
}
